<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <title>DigiTeam - agence digitale sport - Accueil</title>
    <meta name="description" content="Digiteam est une agence de digitale web spécialisée dans l'univers du sport. Basée à Montpellier.">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" href="{{ asset('/images/favicon.ico') }}" />

    <title>{% block title %}DigiTeam{% endblock %}</title>

    {% include 'layout/stylesheets.html.twig' %}

</head>
{% include 'layout/navbar.html.twig' %}

<body data-spy="scroll" data-target=".navbar-collapse" data-offset="100">
<div class="wrapper">

    <!--Accueil Section-->

    {% for message in app.flashes('notice') %}
        <div class="alert alert-success alert-dismissible col-xs-offset-2 col-xs-8 clearfix" style="background-color:white;position:fixed;z-index:10;margin-top:150px;height:100px;">

            <div class="col-xs-11">
                <p style="vertical-align: middle;padding-top: 25px;">{{ message }}</p>
            </div>

            <div class="col-xs-1">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close" style="font-size:60px;font-weight:500;" >
                    <span style="color:black;" aria-hidden="true">&times;</span>
                </button>
            </div>

        </div>
    {% endfor %}

    <section id="accueil" class="home bg-black fix">
        <div class="overlay"></div>
        <div class="container">
            <div class="row">
                <div class="main_home text-center">
                    <div class="col-md-12">
                        <div class="hello">
                            <div class="slid_item">
                                <div class="home_text ">
                                    <h1 class="text-orange">Agence de marketing sportif</h1>
                                    <h3 class="text-white text-uppercase">Création de sites internet et Webmarketing</h3>
                                </div>
                            </div><!-- End off slid item -->
                        </div>
                    </div>
                </div>
            </div>


                <a class="mouse-scroll" href="#agence">
                            <span class="mouse">
                                <span class="mouse-movement"></span>
                            </span>
                    <span class="mouse-message fadeIn">Voir la suite</span> <br />
                    <!--<i class="fa fa-angle-down m-top-10 fadeIn mouse-message"></i>-->
                </a>

            </div><!--End off row-->
        </div><!--End off container -->
    </section> <!--End off accueil Section-->



    <!--agence Section-->
    <section id="agence" class="about bg-orange roomy-80">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div class="main_about text-center">
                        <h1 class="text-black" style="font-size: 45px;">DigiTeam est une agence de création digitale basée à Montpellier</h1>
                        <br/><br/>
                        <h3 class="text-white text-uppercase" style="font-size: 40px">Nous sommes prêts à relever votre nouveau défi !</h3>

                        <a href="{{ path('agenceDigiTeam') }}" class="btn btn-primary m-top-100">Nous découvrir >></a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--End agence Section-->

    <!--Expertises Section-->
    <section id="expertise" class="features bg-white">
        <div class="container">
            <div class="row">
                <div class="main_features fix roomy-70">

                    <div class="col-md-4">
                        <div class="features_item">
                            <div class="head_title">
                                <h2 class="text-uppercase">Notre <strong>Expertise</strong></h2>
                            </div>
                            <div class="featured_content">
                                <p>Nous sommes une agence digitale spécialisée
                                dans le domaine du sport, experte en <strong>création de site web</strong>,
                                <strong>en inbound marketing</strong>,
                                <strong>design </strong>
                                <strong>audit/conseil</strong>.
                                </p>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-6 col-md-offset-1 sm-m-top-50">
                        <div class="features_item2_area">
                            <div class="features_item2 text-center">
                                <div class="divider_horizontal"></div>
                                <div class="divider_vertical"></div>
                                <div class="col-xs-6">
                                    <div class="features_item_text">
                                        <img src="{{ asset('/images/web.png') }}" alt="web" class="img-circle" />
                                        <p class="m-top-20">Web</p>
                                    </div>
                                </div>
                                <div class="col-xs-6">
                                    <div class="features_item_text">
                                        <img src="{{ asset('/images/consulting.png') }}" alt="consulting" class="img-circle" />
                                        <p class="m-top-20">Consulting</p>
                                    </div>
                                </div>
                                <div class="col-xs-6">
                                    <div class="features_item_text m-top-50">
                                        <img src="{{ asset('/images/inbound marketing.png') }}" alt="inbound marketing" class="img-circle" />
                                        <p class="m-top-20">Inbound marketing</p>
                                    </div>
                                </div>
                                <div class="col-xs-6">
                                    <div class="features_item_text m-top-50">
                                        <img src="{{ asset('/images/ux.png') }}" alt="ux design" class="img-circle" />
                                        <p class="m-top-20">Design</p><br/><br/><br/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div align="center">
                        <a href="{{ path ('expertise') }}" class="btn btn-default">En savoir plus >></a>
                    </div>
                    <br/><br/>
                </div>
            </div><!-- End off row -->
        </div><!-- End off container -->
    </section>

    <!--End of Expertises-->




    <!--Portfolio Section-->

    <section id="portfolio" >

        <!-- Portfolio container-->
        <div class="container">
            <div class="row">
                <div class="main-portfolio" style="padding-bottom: 10px;">

                    <div class="col-md-4">
                        <div class="head_title text-left sm-text-center wow fadeInDown">
                            <h2 class="text-uppercase">Nos <strong>Réalisations</strong></h2>
                        </div>
                    </div>

                    <div style="clear: both;"></div>

                    <div class="grid text-center">

                        <div class="grid-item" style="padding-bottom: 5%;">
                            <img alt="Site de la Commune de Bailly" src="{{ asset('/images/Commune-Bailly.png') }}">
                            <div class="grid_hover_area text-center">
                                <div>
                                    <br/>
                                    <h4 class="text-white">Site de la Commune de Bailly</h4>
                                    <p class="text-white">Wordpress</p>
                                    <a href="http://www.elodie-rouel.fr/" class="btn btn-primary">Voir le site >></a>
                                </div>
                            </div><!-- End off grid Hover area -->
                        </div><!-- End off grid item -->

                        <div class="grid-item metalloid " style="padding-top: 2%;">
                            <img alt="Site de la ville de Lille" src="{{ asset('/images/Commune-Lille.png') }}">
                            <div class="grid_hover_area text-center">
                                <div>
                                    <br/><br/>
                                    <h4 class="text-white">Site de la ville de Lille</h4>
                                    <p class="text-white">Wordpress</p>
                                    <a href="http://web-mairie-lille.fr/wordpress/" class="btn btn-primary">Voir le site >></a>
                                </div>
                            </div><!-- End off grid Hover area -->
                        </div><!-- End off grid item -->

                        <div class="grid-item post-transition metal numberGreaterThan50" style="padding-bottom: 5%;">
                            <img alt="Blog Forteroche" src="{{ asset('/images/blogforteroche.png') }}">
                            <div class="grid_hover_area text-center">
                                <div style="padding-bottom: 5%;">
                                    <br/>
                                    <h4 class="text-white">Blog pour un écrivain</h4>
                                    <p class="text-white">Programmation Php</p>
                                    <a href="http://blogforteroche.jutatibouet.fr/'" class="btn btn-primary">Voir le site >></a>
                                </div>
                            </div><!-- End off grid Hover area -->
                        </div><!-- End off grid item -->

                        <div class="grid-item post-transition metal ium" style="padding-top: 2%;" >
                            <img alt="Billetterie du Louvre" src="{{ asset('/images/billetterie-louvre.png') }}">
                            <div class="grid_hover_area text-center">
                                <div>
                                    <br/><br/>
                                    <h4 class="text-white">Billetterie du Musée du Louvre</h4>
                                    <p class="text-white">Symfony 4</p>
                                    <a href="http://billetteriedulouvre.jutatibouet.fr/" class="btn btn-primary">Voir le site >></a>
                                </div>
                            </div><!-- End off grid Hover area -->
                        </div><!-- End off grid item -->

                        <div class="grid-item metal ar" >
                            <img alt="Site de la commune de Savenay" src="{{ asset('/images/Commune-savenay.png') }}">
                            <div class="grid_hover_area text-center">
                                <div>
                                    <br/>
                                    <h4 class="text-white">Site de la commune de Savenay</h4>
                                    <p class="text-white">Wordpress</p>
                                    <a href="http://projet1.jutatibouet.fr/" class="btn btn-primary">Voir le site >></a>
                                </div>
                            </div><!-- End off grid Hover area -->
                        </div><!-- End off grid item -->

                        <div class="grid-item alkali ar" >
                            <img alt="" src="{{ asset('/images/Hockey.png') }}">
                            <div class="grid_hover_area text-center">
                                <div><br/>
                                    <h4 class="text-white">Site Hockey Lille</h4>
                                    <p class="text-white">Wordpress</p>
                                    <a href="http://roller-hockey-asso.fr/" class="btn btn-primary">Voir le site >></a>
                                </div>
                            </div><!-- End off grid Hover area -->
                        </div><!-- End off grid item -->
                    </div>
                    <div style="clear: both;"></div>
                </div>
            </div>
        </div><!-- Portfolio container end -->
    </section><!-- End off portfolio section -->


    <!-- Newsletter -->
    <section id="newsletter" class="about bg-orange roomy-50" style="padding-bottom: 0px">
        <div class="container">
                    <div class="main_about text-center">
                        <h1 class="text-black" style="margin-bottom:20px;">Newsletter</h1>
                                    <div class="col-md-4">
                                        {{ form_start(form) }}
                                        {{ form_errors(form) }}
                                        {{ form_widget(form.prenom, { 'attr': {'class': 'form-control', 'placeholder' :'Votre prénom', 'style': 'width: 20%;min-width: 300px;display: block;margin: 0 auto;margin-top: 25px;margin-bottom: 25px;'} }) }}
                                    </div>
                                    <div class="col-md-4">
                                        {{ form_widget(form.email, { 'attr': {'class': 'form-control', 'placeholder' :'email@exemple.com', 'style': 'width: 20%;min-width: 300px;display: block;margin: 0 auto;margin-top: 25px;margin-bottom: 25px;'} }) }}
                                    </div>
                                    <div class="col-md-4">
                                        {{ form_widget(form.save, {'attr': {'class': 'btn btn-primary', 'style': 'margin-top:25px;'}, 'label': 'Valider >>'}) }}
                                    </div>


                                <div class="col-xs-12" style="margin-top:20px;margin-bottom: 0px">

                                    <label>{{ form_widget(form.caseObligatoire, { 'attr': {'class': '', 'style': ''} }) }} En cochant cette case, j'accepte que mon e-mail soit exploité pour la réception de la newsletter DigiTeam (voir la <a href="{{ path('mentionsLegales') }}">politique de confidentialité</a>)</label>

                                </div>
                        {{ form_widget(form.nom, { 'attr': {'class': 'form-control', 'placeholder' :'Votre nom', 'style': 'visibility:hidden;width: 20%;min-width: 300px;display: block;margin: 0 auto;margin-top: 25px;margin-bottom: 25px;'} }) }}
                                {{ form_rest(form) }}

                                {{ form_end(form) }}


                    </div>
        </div>
        <!-- newsletter / End -->
    </section>


    <a href="#" class="scrollup" style="display: block;"></a>

</div>

{% include 'layout/footer.html.twig' %}

</body>

{% block javascripts %}

    <!-- JS includes -->
    <script type="text/javascript" src="{{ asset('bootstrap/js/vendor/jquery-1.11.2.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap/js/vendor/bootstrap.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap/js/jquery.magnific-popup.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap/js/jquery.easing.1.3.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap/js/slick.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap/js/js.isotope.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap/js/jquery.collapse.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap/js/bootsnav.js') }}"></script>
    <!-- paradise slider js -->
    <script src="{{ asset('https://unpkg.com/masonry-layout@4/dist/masonry.pkgd.min.js') }}"></script>

    <script type="text/javascript" src="{{ asset('bootstrap/js/plugins.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap/js/main.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap/js/vendor/modernizr-2.8.3-respond-1.4.2.min.js') }}"></script>
{% endblock %}


</html>